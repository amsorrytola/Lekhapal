datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model TableData {
  id Int @id @default(autoincrement())
  title String
  columns String[] // Prisma supports native string[] on Postgres; for SQLite use Json
  rows Json // 2D array stored as JSON
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
